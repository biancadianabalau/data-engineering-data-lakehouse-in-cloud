CREATE SCHEMA IF NOT EXISTS bronze;

DROP TABLE IF EXISTS bronze.customers;

CREATE TABLE bronze.customers
USING PARQUET
LOCATION 's3://etl-project3-data-warehouse-in-cloud/bronze/customers/customers.parquet';

DESCRIBE bronze.customers;
SELECT * FROM bronze.customers LIMIT 10;



DROP TABLE IF EXISTS bronze.products;
CREATE TABLE IF NOT EXISTS bronze.products
USING PARQUET
LOCATION 's3://etl-project3-data-warehouse-in-cloud/bronze/products/products.parquet';

DESCRIBE bronze.products;
SELECT * FROM bronze.products LIMIT 10;


DROP TABLE IF EXISTS bronze.product_category_translation;
CREATE TABLE IF NOT EXISTS bronze.product_category_translation
USING PARQUET
LOCATION 's3://etl-project3-data-warehouse-in-cloud/bronze/product_category_translation/product_category_translation.parquet';

